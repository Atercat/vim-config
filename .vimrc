set guifont=Cascadia_Mono:h11
set langmenu=en_US.UTF-8
set encoding=UTF-8
language en
colorscheme desert
syntax on
set autoindent
set backspace=indent
set number
set tabstop=2 softtabstop=0 expandtab shiftwidth=2 smarttab
set backupdir=%TEMP%
set directory=%TEMP%
au BufReadPost,BufNewFile *.v8i set syntax=dosini
